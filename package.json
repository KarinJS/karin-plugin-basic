{
  "name": "@karinjs/plugin-basic",
  "version": "0.0.2",
  "description": "karin plugin for basic functions",
  "homepage": "https://github.com/KarinJS/karin-plugin-basic",
  "bugs": {
    "url": "https://github.com/KarinJS/karin-plugin-basic/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarinJS/karin-plugin-basic.git"
  },
  "author": "shijin",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "config",
    "resources",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "pr": "node lib/cli/pr.js",
    "pub": "npm publish --access public",
    "sort": "npx sort-package-json",
    "dev": "karin ts"
  },
  "devDependencies": {
    "@types/node": "^20.17.8",
    "eslint": "^9.7.0",
    "neostandard": "^0.12.0",
    "node-karin": "0.12.25-beta.pr.206.1-1734651009",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "lib/**/*",
      "@karinjs/**/*"
    ]
  }
}